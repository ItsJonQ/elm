{
  "name": "@itsjonq/elm",
  "version": "0.0.3",
  "description": "A collection of handy React Elements",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "sideEffects": false,
  "private": false,
  "scripts": {
    "prestart": "zero prestart",
    "start": "npm run storybook",
    "coverage": "nyc report --temp-directory=coverage --reporter=text-lcov | coveralls",
    "build:es": "BUILD_FORMAT=es zero build -d dist/es",
    "build:cjs": "BUILD_FORMAT=cjs zero build -d dist/cjs",
    "build": "npm run build:cjs && npm run build:es -- --no-clean",
    "open:coverage": "open ./coverage/lcov-report/index.html",
    "lint": "zero lint",
    "dev": "zero test",
    "test": "zero test --coverage",
    "test:coverage": "zero test --coverage",
    "format": "zero format",
    "validate": "zero validate",
    "release": "zero release",
    "version": "npm run build",
    "precommit": "zero pre-commit",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "Jon Quach <hello@jonquach.com> (https://jonquach.com)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itsjonq/elm.git"
  },
  "bugs": {
    "url": "https://github.com/itsjonq/elm/issues"
  },
  "homepage": "https://github.com/itsjonq/elm#readme",
  "keywords": [
    "react",
    "components",
    "elements",
    "common",
    "ui",
    "kit",
    "base",
    "view",
    "styled-components",
    "emotion",
    "css-in-js",
    "css"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.8.x"
  },
  "devDependencies": {
    "@babel/core": "7.7.4",
    "@itsjonq/zero": "4.1.8",
    "@storybook/addon-actions": "5.2.6",
    "@storybook/addon-links": "5.2.6",
    "@storybook/addons": "5.2.6",
    "@storybook/react": "5.2.6",
    "babel-loader": "8.0.6",
    "coveralls": "3.0.9",
    "nyc": "14.1.1",
    "react": "16.12.0",
    "react-dom": "16.12.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@itsjonq/cyan": "0.15.1",
    "@sindresorhus/is": "1.2.0",
    "dash-get": "1.0.2",
    "json2mq": "0.2.0",
    "styled-view": "0.0.7"
  }
}